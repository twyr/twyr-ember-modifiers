{
	"name": "@twyr/ember-utility-modifiers",
	"displayName": "Ember Utility Modifiers",
	"description": "Ember v4.0+ implementation of utility modifiers that are generally useful",
	"homepage": "https://github.com/twyr/twyr-utility-modifiers#readme",
	"license": "MITNFA",
	"readme": "README.md",
	"version": "0.5.1",
	"keywords": [
		"ember-addon",
		"ember"
	],
	"author": {
		"name": "Vish Desai",
		"email": "shadyvd@hotmail.com",
		"url": "https://github.com/shadyvd"
	},
	"contributors": [
		{
			"name": "Vish Desai",
			"email": "shadyvd@hotmail.com",
			"url": "https://github.com/shadyvd"
		}
	],
	"maintainers": [
		{
			"name": "Vish Desai",
			"email": "shadyvd@hotmail.com",
			"url": "https://github.com/shadyvd"
		}
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/twyr/twyr-utility-modifiers.git"
	},
	"bugs": {
		"url": "https://github.com/twyr/twyr-utility-modifiers/issues"
	},
	"exports": {
		"./*": "./dist/*",
		"./addon-main.js": "./addon-main.js"
	},
	"directories": {
		"doc": "doc",
		"test": "tests"
	},
	"files": [
		"addon-main.js",
		"dist"
	],
	"dependencies": {
		"@embroider/addon-shim": "1.6.0",
		"ember-debug-logger": "2.1.1",
		"ember-modifier": "3.2.7",
		"ember-source": "4.3.0",
		"uuid": "8.3.2"
	},
	"devDependencies": {
		"@babel/core": "7.17.9",
		"@babel/eslint-parser": "7.17.0",
		"@babel/plugin-proposal-class-properties": "7.16.7",
		"@babel/plugin-proposal-decorators": "7.17.9",
		"@babel/plugin-syntax-decorators": "7.17.0",
		"@embroider/addon-dev": "1.6.0",
		"@rollup/plugin-babel": "5.3.1",
		"ember-cli": "4.3.0",
		"eslint": "8.14.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-ember": "10.6.0",
		"eslint-plugin-json": "3.1.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-prettier": "4.0.0",
		"prettier": "2.6.2",
		"prettier-plugin-jsdoc": "0.3.38",
		"rollup": "2.70.2"
	},
	"scripts": {
		"build": "npx rollup --config",
		"build:clean": "mkdir -p ./dist && rm -rf ./dist && npm run build && npm run lint:fix",
		"lint": "npm run lint:js",
		"lint:fix": "npm run lint:js:fix",
		"lint:js": "npx prettier './**/*.{js,json}' --no-error-on-unmatched-pattern --list-different && npx eslint './**/*.{js,json}' --no-error-on-unmatched-pattern --cache",
		"lint:js:fix": "npx prettier './**/*.{js,json}' --no-error-on-unmatched-pattern --write && npx eslint './**/*.{js,json}' --no-error-on-unmatched-pattern --cache --fix",
		"lint-staged": "npx lint-staged --allow-empty",
		"prepare": "npm run build:clean",
		"start": "npx rollup --config --watch --no-watch.clearScreen",
		"start:clean": "mkdir -p ./dist && rm -rf ./dist && npm run lint:fix && npm run start",
		"test": "echo 'Addon does not have tests, run tests in test-app'"
	},
	"engines": {
		"node": ">= 14"
	},
	"ember": {
		"edition": "octane"
	},
	"ember-addon": {
		"version": 2,
		"type": "addon",
		"main": "addon-main.js",
		"build": "build-injections.js",
		"app-js": {
			"./modifiers/capture-on.js": "./dist/_app_/modifiers/capture-on.js",
			"./modifiers/css-style.js": "./dist/_app_/modifiers/css-style.js",
			"./modifiers/has-class-if.js": "./dist/_app_/modifiers/has-class-if.js",
			"./modifiers/not-on.js": "./dist/_app_/modifiers/not-on.js",
			"./modifiers/on-args-change.js": "./dist/_app_/modifiers/on-args-change.js",
			"./modifiers/on-mutation.js": "./dist/_app_/modifiers/on-mutation.js",
			"./modifiers/on-resize.js": "./dist/_app_/modifiers/on-resize.js",
			"./modifiers/on-style-change.js": "./dist/_app_/modifiers/on-style-change.js",
			"./modifiers/requires-id.js": "./dist/_app_/modifiers/requires-id.js",
			"./modifiers/store-element.js": "./dist/_app_/modifiers/store-element.js",
			"./services/element-computed-style-watcher.js": "./dist/_app_/services/element-computed-style-watcher.js",
			"./services/element-mutation-watcher.js": "./dist/_app_/services/element-mutation-watcher.js",
			"./services/element-resize-watcher.js": "./dist/_app_/services/element-resize-watcher.js",
			"./utils/check-browser-features.js": "./dist/_app_/utils/check-browser-features.js"
		}
	}
}
